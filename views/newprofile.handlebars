<form class="profile">
  <div class="new_container">
    <h1>Create profile</h1>
    <h4>Required information</h4>
    <!-- First Name Last Name -->
    <form class="profile">
      <div class="row-1">
        <div class="col">
          <input type="text" class="form-control  first-name" placeholder="First name">
        </div>
        <div class="col">
          <input type="text" class="form-control last-name" placeholder="Last name">
        </div>
      </div>
      <br>
      <!-- Relationship -->
      <label class="relationship">Relationship
      </label>
      <select name="relationship" id="relationship">
        <option value=""></option>
        <option value="Parent/Guardian">Parent/Guardian</option>
        <option value="Sibling">Sibling</option>
        <option value="Child">Child</option>
        <option value="Aunt/Uncle">Aunt/Uncle</option>
        <option value="Someone Special">Someone Special</option>
        <option value="It's_Complicated">It's Complicated</option>
        <option value="Partner">Partner</option>
      </select>
      </label>
      <br>
